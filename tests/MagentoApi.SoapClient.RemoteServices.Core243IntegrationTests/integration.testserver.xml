<?xml version="1.0" encoding="utf-8"?>
<!-- 

* Copy this file to integration.testserver.ignore.xml
* Adjust settings to your integration server 
* Start tests

-->
<settings>
  <baseEndpointAddress>http://www.example.com/shop</baseEndpointAddress>
  <userAgent>test</userAgent>
  
  <token>
    <type>admin</type>
    <requestUserName>my-admin-name</requestUserName>
    <requestPassword>my-admin-password</requestPassword>
    <expirationPeriod>01:23:45</expirationPeriod>
    <httpBasicAuthenticationUserName>http-auth-user</httpBasicAuthenticationUserName>
    <httpBasicAuthenticationPassword>http-auth-password</httpBasicAuthenticationPassword>
  </token>
  
  <trace>
    <enable>true</enable>
    <messagesFolder>tempfolder</messagesFolder>
  </trace>
  
  <http>
    <sendTimeout>00:00:30</sendTimeout>
    <transportMaxReceivedMessageSize>10.000.077</transportMaxReceivedMessageSize>
  </http>
</settings>